#include <stdio.h>

int main()
{
    int n,i,m=0,flag=0;
    printf("Enter the number : ");
    scanf("%d",&n);
    if(n==0 || n==1)
    printf("there the not composite number or ");
    m=n/2;
    for(i=2;i<=m;i++)
    {
      if(n%i==0)
      {
          printf("composite number");
          flag=1;
          break;
         
      }
    }
    if(flag==0 )
    {
        printf("prime number");
      
    }
    return 0;
}
